//tlc case: 21=7
function signed fib(signed a, signed b, signed c, signed n) {
  if(n < 2)
    1;
  else
    fib(0, 0, 0, n-1) + fib(0, 0, 0, n-2);
}
signed n;
fib(0, 0, 0, n);
